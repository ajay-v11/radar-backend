# API Keys - Add your API keys here (at least one required)
OPENAI_API_KEY=sk-your-openai-api-key-here
ANTHROPIC_API_KEY=sk-ant-your-anthropic-api-key-here
GEMINI_API_KEY=your-gemini-api-key-here

# Groq Cloud API Key (for Llama 4 Scout)
# Get your key from: https://console.groq.com/keys
GROK_API_KEY=gsk-your-groq-api-key-here

# OpenRouter API Key (for Grok 4 Fast, DeepSeek)
# Get your key from: https://openrouter.ai/keys
OPEN_ROUTER_API_KEY=sk-or-v1-your-openrouter-api-key-here

# Firecrawl API Key (Required for enhanced industry detection)
FIRECRAWL_API_KEY=fc-your-firecrawl-api-key-here

# Application Settings (Optional - defaults provided)
APP_NAME=AI Visibility Scoring System
APP_VERSION=1.0.0
DEBUG=false

# Model Settings (Optional - defaults provided)
# Cost-effective models are used by default
CHATGPT_MODEL=gpt-3.5-turbo
CLAUDE_MODEL=claude-3-haiku-20240307
GEMINI_MODEL=gemini-2.5-flash-lite
GROQ_LLAMA_MODEL=llama-3.1-8b-instant
OPENROUTER_GROK_MODEL=x-ai/grok-4.1-fast
OPENROUTER_DEEPSEEK_MODEL=deepseek/deepseek-chat-v3-0324:free

# Industry Analysis Model (Used for enhanced industry detection)
INDUSTRY_ANALYSIS_MODEL=gpt-4o-mini

# Query Settings (Optional - defaults provided)
NUM_QUERIES=20

# ChromaDB Settings (Optional - defaults provided)
CHROMA_HOST=localhost
CHROMA_PORT=8001
CHROMA_COLLECTION_COMPANIES=companies
CHROMA_COLLECTION_COMPETITORS=competitors

# Redis Settings (Optional - defaults provided)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_CACHE_TTL=3600
REDIS_MAX_CONNECTIONS=10
